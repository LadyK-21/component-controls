{
  "name": "@component-controls/jsdoc-extract",
  "version": "3.14.4",
  "description": "jsdoc type comments extract form js and ts files",
  "keywords": [
    "jsdoc",
    "tsdoc",
    "javascript"
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "yarn cross-env NODE_ENV=production rollup -c",
    "dev": "yarn cross-env NODE_ENV=development rollup -cw",
