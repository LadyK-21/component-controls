    "directory": "misc/jsdoc-extract"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.12.5",
    "@babel/traverse": "^7.12.5",
    "@component-controls/follow-imports": "^3.14.4",
    "comment-parser": "^1.1.5",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@babel/types": "^7.12.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "authors": [
    "Atanas Stoyanov"
  ],
  "jest": {
    "roots": [
      "./test"
